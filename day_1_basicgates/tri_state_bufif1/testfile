#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ba8fd7fb2c0 .scope module, "bufif1_tb" "bufif1_tb" 2 1;
 .timescale 0 0;
v0x5ba8fd80c990_0 .var "a", 0 0;
v0x5ba8fd80ca60_0 .var "control", 0 0;
v0x5ba8fd80cb30_0 .net8 "y", 0 0, L_0x5ba8fd80cc30;  1 drivers, strength-aware
S_0x5ba8fd7fb450 .scope module, "inst" "buf_if1" 2 5, 3 1 0, S_0x5ba8fd7fb2c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "control";
    .port_info 2 /OUTPUT 1 "y";
L_0x5ba8fd80cc30 .functor BUFIF1 1, v0x5ba8fd80c990_0, v0x5ba8fd80ca60_0, C4<0>, C4<0>;
v0x5ba8fd7fb680_0 .net "a", 0 0, v0x5ba8fd80c990_0;  1 drivers
v0x5ba8fd80c7b0_0 .net "control", 0 0, v0x5ba8fd80ca60_0;  1 drivers
v0x5ba8fd80c870_0 .net8 "y", 0 0, L_0x5ba8fd80cc30;  alias, 1 drivers, strength-aware
    .scope S_0x5ba8fd7fb2c0;
T_0 ;
    %vpi_call 2 9 "$monitor", "%0t|a=%b|control=%b|y=%b|", $time, v0x5ba8fd80c990_0, v0x5ba8fd80ca60_0, v0x5ba8fd80cb30_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ba8fd80c990_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ba8fd80ca60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ba8fd80c990_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ba8fd80ca60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ba8fd80c990_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ba8fd80ca60_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bufif1_tb.v";
    "bufif1.v";
