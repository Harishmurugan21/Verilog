#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x63a1c1c342f0 .scope module, "nor_gate_tb" "nor_gate_tb" 2 1;
 .timescale 0 0;
v0x63a1c1c465e0_0 .var "a", 0 0;
v0x63a1c1c466b0_0 .var "b", 0 0;
v0x63a1c1c46780_0 .net "y", 0 0, L_0x63a1c1c46880;  1 drivers
S_0x63a1c1c34480 .scope module, "dut" "nor_gate" 2 5, 3 1 0, S_0x63a1c1c342f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x63a1c1c46880 .functor NOR 1, v0x63a1c1c465e0_0, v0x63a1c1c466b0_0, C4<0>, C4<0>;
v0x63a1c1bfb490_0 .net "a", 0 0, v0x63a1c1c465e0_0;  1 drivers
v0x63a1c1c46400_0 .net "b", 0 0, v0x63a1c1c466b0_0;  1 drivers
v0x63a1c1c464c0_0 .net "y", 0 0, L_0x63a1c1c46880;  alias, 1 drivers
    .scope S_0x63a1c1c342f0;
T_0 ;
    %vpi_call 2 9 "$monitor", "%0t|a=%b|b=%b|y=%b", $time, v0x63a1c1c465e0_0, v0x63a1c1c466b0_0, v0x63a1c1c46780_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63a1c1c465e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63a1c1c466b0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63a1c1c465e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63a1c1c466b0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63a1c1c465e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63a1c1c466b0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63a1c1c465e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63a1c1c466b0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x63a1c1c342f0;
T_1 ;
    %vpi_call 2 19 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "nor_gate_tb.v";
    "nor_gate.v";
