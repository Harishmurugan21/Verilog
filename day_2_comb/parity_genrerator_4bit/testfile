#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59351f03c200 .scope module, "parity_generator" "parity_generator" 2 2;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /OUTPUT 1 "y";
o0x75b8287a0018 .functor BUFZ 4, C4<zzzz>; HiZ drive
v0x59351f03c520_0 .net "a", 3 0, o0x75b8287a0018;  0 drivers
v0x59351f04e400_0 .net "y", 0 0, L_0x59351f04eb10;  1 drivers
L_0x59351f04eb10 .reduce/xor o0x75b8287a0018;
S_0x59351f03c390 .scope module, "parity_generator_tb" "parity_generator_tb" 3 1;
 .timescale 0 0;
v0x59351f04e980_0 .var "a", 3 0;
v0x59351f04ea40_0 .net "y", 0 0, v0x59351f04e860_0;  1 drivers
S_0x59351f04e520 .scope module, "inst" "parity_generator_behavioural" 3 6, 2 11 0, S_0x59351f03c390;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /OUTPUT 1 "y";
v0x59351f04e760_0 .net "a", 3 0, v0x59351f04e980_0;  1 drivers
v0x59351f04e860_0 .var "y", 0 0;
E_0x59351f003a70 .event anyedge, v0x59351f04e760_0;
    .scope S_0x59351f04e520;
T_0 ;
    %wait E_0x59351f003a70;
    %load/vec4 v0x59351f04e760_0;
    %xor/r;
    %store/vec4 v0x59351f04e860_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x59351f03c390;
T_1 ;
    %vpi_call 3 9 "$monitor", "%0t|a=%b|y=%b|", $time, v0x59351f04e980_0, v0x59351f04ea40_0 {0 0 0};
    %vpi_call 3 10 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 3 11 "$dumpvars" {0 0 0};
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x59351f04e980_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x59351f04e980_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x59351f04e980_0, 0, 4;
    %delay 5, 0;
    %vpi_call 3 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "parity_generate.v";
    "parity_generator_tb.v";
